{
  "name": "@rpearce/hex",
  "version": "0.3.1",
  "description": "Hexadecimal color code & RGBa utility functions",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:rpearce/hex.git"
  },
  "homepage": "https://github.com/rpearce/hex",
  "bugs": "https://github.com/rpearce/hex/issues",
  "author": "Robert Pearce <me@robertwpearce.com>",
  "contributors": [
    "Robert Pearce <me@robertwpearce.com> (https://robertwpearce.com)"
  ],
  "license": "ISC",
  "keywords": [
    "hex",
    "hexadecimal",
    "hex-to-rgb",
    "hex-to-rgba",
    "rgba-to-hex",
    "rgb-to-hex"
  ],
  "tags": [
    "hex",
    "hexadecimal",
    "hex-to-rgb",
    "hex-to-rgba",
    "rgba-to-hex",
    "rgb-to-hex"
  ],
  "files": [
    "AUTHORS",
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "run-s clean build:js",
    "build:js": "rollup -c rollup.config.js",
    "clean": "rm -rf dist",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:report": "nyc report --reporter=lcov && echo \"view coverage: ./coverage/lcov-report/index.html\"",
    "lint": "eslint .",
    "prepublishOnly": "run-p lint test && npm run build",
    "test": "nyc ava --verbose"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "all-contributors-cli": "6.13.0",
    "ava": "2.4.0",
    "babel-core": "7.0.0-bridge.0",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.7",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "rollup": "1.31.1",
    "rollup-plugin-babel": "4.3.3"
  }
}
